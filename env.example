# Environment variables for Symfony application

# Application environment
APP_ENV=prod
APP_DEBUG=0

# Application secret (will be generated by Render)
APP_SECRET=your-secret-here

# Database configuration (SQLite)
DATABASE_URL="sqlite:///%kernel.project_dir%/var/database.sqlite"

# Mailer configuration (disabled for now)
MAILER_DSN=null://null

# Cache configuration
CACHE_DRIVER=file

# Session configuration
SESSION_DRIVER=file

# Logging configuration
LOG_LEVEL=warning
